	<script src="js/jquery.nicescroll.min.js"></script>
	<script>
		$(document).ready(function () {
			$('html').niceScroll({
				touchbehavior: false,
				cursorcolor: "#E9204F",
				cursoropacitymax: 0.5,
				cursorwidth: 7,
				background: "#f6f6f6",
				autohidemode: false
			});
		
			$(".scroll").click(function () {
				$('.scroll').addClass('slide-out-top');
				$("html,body").animate({ scrollTop: $(".thetop").offset().top }, 1000);
				
				setTimeout(function(){
					$('.scroll').removeClass('slide-out-top'); 
				}, 1500);
				
				return false;
			});
		
			var imageSwiper = new Swiper('.image-swiper', {
				slidesPerView: 1,
				autoplay: 4000,
				loop: true,
				speed: 2000,
				grabCursor: true,
				allowTouchMove: true
			});
		
			var swiper = new Swiper('.content-swiper', {
				pagination: '.swiper-pagination',
				slidesPerView: 1,
				paginationClickable: true,
				loop: true,
				allowTouchMove: true,
				paginationBulletRender: function (index, className) {
					var tabsName = ['1<sup>st</sup>&nbsp;Level', '2<sup>nd</sup>&nbsp;Level', '3<sup>rd</sup>&nbsp;Level'];
					if (index === (tabsName.length - 1)) {
						return '<span class="' + className + '">' +
							tabsName[index] + '</span>' +
							'<div class="active-mark "></div>';
					}
					return '<span class="' + className + '">' + tabsName[index] + '</span>';
				}
			});
		});
		
		$(window).scroll(function () {
			if ($(this).scrollTop() > 50) {
				$('.scrolltop:hidden').stop(true, true).fadeIn();
			}
			else {
				$('.scrolltop').stop(true, true).fadeOut();
			}
		});
	</script>
